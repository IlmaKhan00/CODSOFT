a=int(input("enter a number in a: "))

b=int(input("enter a number in b: "))

operation=input("Select the operator [+ - * / %] : ")

result=0

if operation=='+':
    result=a+b

elif operation=='-':
    result=a-b

elif operation=='*':
    result=a*b

elif operation=='/':
    result=a/b

elif operation=='%':
    result=a%b

else:
    print("Invalid Operator......")

print(a,operation,b,'=',result)    
       
